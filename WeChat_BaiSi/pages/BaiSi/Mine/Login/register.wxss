.inputStyle{
  display: flex;
  flex-direction: column;

  margin: 60px 30px 10px 30px;

  border-radius: 5px;

  background-color: white;
}
.inputStyle .nameStyle{
  height: 40px;

  font-size: 13px;

  margin-left: 10px;
  margin-right: 10px;
}
.inputStyle .lineStyle{
  width: 100%;
  height: 1rpx;

  background-color: lightgray;
}
.inputStyle .passwordStyle{
  height: 40px;

  font-size: 13px;

  margin-left: 10px;
  margin-right: 10px;
}

.registerStyle{
  margin: 10px 30px 10px 30px;

  height: 40px;

  background-color: red;

  border-radius: 5px;

  color: white;
}
.registerStyle:active{
  background-color: Brown;
}